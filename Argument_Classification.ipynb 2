        "def f_importances(coef, names):\n",
        "    imp = coef[0]\n",
        "    imp,names = zip(*sorted(zip(imp,names)))\n",
        "    plt.barh(range(len(names)), imp, align='center')\n",
        "    plt.yticks(range(len(names)), names)\n",
        "    plt.show()\n"
      ],
      "execution_count": 157,
